        <input type="text" name="grade" />
        <label for="Organic">Organic</label>
        <input type="text" name="organic" />

        <label for="RR">RR</label>
        <input type="text" name="rr" />
        <label for="dfd">dfd</label>
        <input type="text" name="dfd" />
        <label for="freshness">freshness</label>
        <input type="text" name="freshness" id="" />
        <label for="seasonal">Seasonal</label>
        <input type="text" name="seasonal" id="" />
        <label for="otipy">Otipy Certified </label>
        <input type="text" name="otipy" id="" />
        <label for="uniqueCustomer">Unique Customer </label>
        <input type="text" name="unique" id="" />
