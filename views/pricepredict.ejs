  <body>
    <div class="container">
      <h1 class="title">Price Pridiction</h1>
      <form class="form" action="/price_predict" method="post">
        <label for="item">Iteam</label>
        <input type="text" name="item" />
        <label for="state">State</label>
        <input type="text" name="state" />
